{
  "name": "electron-react-hot-ts-boilerplate",
  "version": "2.0.0",
  "description": "Live Editing Environment for Electron Apps using React, React-Hot-Loader, Typescript and Webpack",
  "main": "./dist/main.js",
  "scripts": {
    "start": "electron ./dist/main.js",
    "devstart": "cross-env ELECTRON_DEV=dev electron ./dist/main.js",
    "dev": "webpack --config ./webpack.config.dev.js",
    "build": "webpack --config ./webpack.config.prod.js",
    "server": "yarn run dev && webpack-dev-server --config ./webpack.config.server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "webpack3",
    "webpack",
    "react-hot-loader-3",
    "hot-reloading"
  ],
  "author": "Tarak Sharma",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quantummkv/electron-react-hot-ts-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/quantummkv/electron-react-hot-ts-boilerplate/issues"
  },
  "homepage": "https://github.com/quantummkv/electron-react-hot-ts-boilerplate#readme",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^16.0.27",
    "@types/react-dom": "^16.0.3",
    "@types/react-hot-loader": "^3.0.5",
    "@types/webpack-env": "^1.13.2",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "electron": "^1.7.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "react-hot-loader": "^3.1.3",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.0",
    "ts-loader": "^3.2.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.5",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
